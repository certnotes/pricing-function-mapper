{
  "seed": 42,
  "budget": 180,
  "init_n": 70,
  "batch_size": 15,
  "pool_size": 10000,
  "output_dir": "outputs",
  "run_id": "segment_low_risk_high_premium",
  "output_csv": "comp_car_quotes_advanced.csv",
  "output_metadata_json": "run_metadata.json",
  "state_path": "run_state.json",
  "engine_path": "pricing_engine.pkl",
