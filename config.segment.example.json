    "claims_5y": {
      "max": 1
    },
    "convictions_5y": {
      "max": 0
    },
    "postcode_risk": {
      "max": 0.35
    },
    "parking": {
      "in": ["garage", "driveway"]
    },
    "theft_risk": {
      "max": 0.35
    }
  },
  "segment_target_weight": 0.45,
  "segment_sigma_weight": 0.25,
  "segment_min_candidates": 500,
  "segment_pool_max_tries": 5,
  "quote_provider": null,
  "domain_overrides": {
    "continuous": {},
    "integers": {},
    "categorical": {}
  }
